{
  "format_version": "1.20.30",
  "minecraft:attachable": {
    "description": {
      "identifier": "demo:anchor_hook",
      "item": {
        "demo:anchor_hook": "query.is_owner_identifier_any('minecraft:player')"
      },
      "materials": {
        "default": "entity",
        "enchanted": "entity_alphatest_glint"
      },
      "textures": {
        "default": "textures/items/anchor_hook",
        "enchanted": "textures/misc/enchanted_item_glint"
      },
      "geometry": {
        "default": "geometry.anchor_hook"
      },
      "animations": {
        "wield": "controller.animation.anchor.wield",
        "wield_first_person": "animation.anchor.wield_first_person",
        "wield_first_person_raise": "animation.anchor.wield_first_person_raise",
        "wield_first_person_raise_shake": "animation.anchor.wield_first_person_raise_shake",
        "wield_first_person_riptide": "animation.anchor.wield_first_person_riptide",
        "wield_third_person": "animation.anchor.wield_third_person",
        "wield_third_person_raise": "animation.anchor.wield_third_person_raise"
      },
      "scripts": {
        "pre_animation": [
          "variable.charge_amount = math.clamp((query.main_hand_item_max_duration - (query.main_hand_item_use_duration - query.frame_alpha + 1.0)) / 10.0, 0.0, 1.0f);"
        ],
        "animate": [
          "wield"
        ]
      },
      "render_controllers": ["controller.render.item_default"]
    }
  }
}
